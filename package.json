{
  "name": "ai-orchestra",
  "version": "0.6.0",
  "description": "Autonomous multi-LLM development system with distributed agent orchestration",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "node --watch server.js",
    "test": "c8 --reporter=text --reporter=html node --test tests/**/*.test.js",
    "test:unit": "node --test tests/unit/**/*.test.js",
    "test:watch": "node --test --watch tests/**/*.test.js",
    "test:coverage": "c8 --reporter=text --reporter=html --reporter=lcov node --test tests/**/*.test.js",
    "test:integration": "node tests/integration_tests.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "docker:build": "docker build -t ai-orchestra .",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "setup": "node scripts/setup.js"
  },
  "keywords": [
    "ai",
    "llm",
    "agents",
    "orchestration",
    "multi-agent",
    "autonomous",
    "development"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.68.0",
    "@octokit/rest": "^20.0.2",
    "axios": "^1.6.7",
    "better-sqlite3": "^9.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ollama": "^0.5.0",
    "openai": "^4.28.0",
    "pg": "^8.11.3",
    "prom-client": "^15.1.0",
    "rate-limiter-flexible": "^5.0.0",
    "redis": "^4.6.12",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "autocannon": "^8.0.0",
    "c8": "^9.1.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "nock": "^13.5.6",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.4",
    "sinon": "^17.0.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
