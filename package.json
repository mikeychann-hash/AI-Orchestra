{
  "name": "ai-orchestra",
  "version": "0.6.0",
  "description": "Autonomous multi-LLM development system with distributed agent orchestration",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "node --watch server.js",
    "test": "node --test tests/**/*.test.js",
    "test:integration": "node tests/integration_tests.js",
    "docker:build": "docker build -t ai-orchestra .",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "setup": "node scripts/setup.js"
  },
  "keywords": [
    "ai",
    "llm",
    "agents",
    "orchestration",
    "multi-agent",
    "autonomous",
    "development"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "ws": "^8.16.0",
    "better-sqlite3": "^9.4.0",
    "dotenv": "^16.4.1",
    "axios": "^1.6.7",
    "openai": "^4.28.0",
    "ollama": "^0.5.0",
    "@octokit/rest": "^20.0.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "prom-client": "^15.1.0",
    "express-rate-limit": "^7.1.5",
    "rate-limiter-flexible": "^5.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "pg": "^8.11.3",
    "redis": "^4.6.12"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.4",
    "nodemon": "^3.0.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
